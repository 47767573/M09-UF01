import java.util.Random;
import java.util.Calendar;


public class Procesos extends Thread {
	
	private String nombre;
	private int tipo;
	private Calendar tiempo;
	private long inicio = tiempo.getTimeInMillis();
	
	public Procesos(String nombre, int tipo) {
		this.nombre = nombre;
		this.tipo = tipo;
		inicio = tiempo.;
	}

	
	@Override
	public void run (){
		Random rnd = new Random();	
		
		for (int i=0; i<3 ; i++){
			
			if (tipo == 1){
				long fin = tiempo.getTimeInMillis();
				int num = rnd.nextInt(10);
				System.out.println(nombre+"--> "+num+" <"+(fin - inicio)+">");
			}else if (tipo == 2){
				int num = rnd.nextInt(10);
				System.out.println(nombre+"--> "+num);
				//long tiempoTotal = (tiempoFin - tiempo)/1000;
			}
			
		}
	}
	
	
	
	
}
