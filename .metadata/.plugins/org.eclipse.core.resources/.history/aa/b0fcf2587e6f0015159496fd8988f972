import java.util.Random;


public class Procesos extends Thread {
	
	private String nombre;
	
	public Procesos(String nombre) {
		this.nombre = nombre;
		
	}

	
	@Override
	public void run (){
		Random rnd = new Random();	
		
		for (int i=0; i<3 ; i++){
			
			int num = rnd.nextInt(10);
			System.out.println(nombre+"--> "+num);
		}
	}
	
	
	
	
}
